.sleeve {
  font-family: sleeve !important;
  color: #157658 !important;
}

.container {
  display: flex;
  flex-direction: row;
  height: 100rpx;
  width: 100%;
  background-color: #ffffff;
}
.icons {
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  align-items: center;
  justify-content: space-around;
  width: 230rpx;
  padding: 0 22rpx 0 16rpx;
  height: 100%;
  color: #999999;
}
.icons > view {
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.icons > view:nth-child(2) {
  padding-top: 2rpx;
}
.icons text {
  font-size: 20rpx;
}
.btn-font {
  color: #ffffff;
  font-size: 32rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.buy {
  background-color: #157658;
  width: 260rpx;
  height: 100%;
}
.cart {
  background-color: #333333;
  width: 260rpx;
  height: 100%;
}